<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MagicTabLayout">
        <attr name="enablePivotScroll" format="boolean" />
        <attr name="isFollowTouch" format="boolean" />
        <attr name="isIndicatorOnTop" format="boolean" />
        <attr name="isAlignBaseLine" format="boolean" />
        <attr name="isSkimOverEnable" format="boolean" />
        <attr name="isSmoothScrollEnable" format="boolean" />
        <attr name="scrollPivotX" format="float" />
        <attr name="tabSelectedTextColor" format="color" />
        <attr name="tanUnselectedTextColor" format="color" />
        <attr name="tabSelectedTextSize" format="dimension" />
        <attr name="tabUnSelectedTextSize" format="dimension" />
        <attr name="indicatorColor" format="color" />
        <attr name="indicatorWidth" format="dimension" />
        <attr name="showIndicator" format="boolean" />
        <attr name="tabBoldMode" format="enum">
            <enum name="select" value="0" />
            <enum name="unselect" value="1" />
            <enum name="both" value="2" />
        </attr>
        <attr name="tabNavigateMode" format="enum">
            <enum name="scrollable" value="0" />
            <enum name="fixed" value="1" />
        </attr>
    </declare-styleable>
</resources>